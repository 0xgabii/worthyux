{
  "name": "worthyux",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/gomonk3037/worthyux",
  "author": "Eunseok Go",
  "license": "MIT",
  "scripts": {
    "start": "next",
    "build": "next build && next export"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "^10.0.1",
    "classnames": "^2.2.6",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.0",
    "mobx": "4",
    "mobx-react": "^5.4.3",
    "next": "^7.0.2",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension": 0,
      "react/prefer-stateless-function": 0,
      "react/prop-types": 0,
      "import/prefer-default-export": 0,
      "react/button-has-type": 0,
      "no-return-assign": 0
    },
    "env": {
      "browser": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "legacyDecorators": true
      }
    }
  }
}
